<entities xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:noNamespaceSchemaLocation="urn:magento:mftf:DataGenerator/etc/dataProfileSchema.xsd">
    <entity name="GoogleAnalyticsDisabledByDefault" type="config">
        <data key="path">google/analytics/active</data>
        <data key="value">0</data>
    </entity>
    <entity name="GoogleAnalyticsTypeByDefault" type="config">
        <data key="path">google/analytics/type</data>
        <data key="value">universal</data>
    </entity>
    <entity name="GoogleAnalyticsEnabled" type="config">
        <data key="path">google/analytics/active</data>
        <data key="value">1</data>
    </entity>
    <entity name="GoogleAnalyticsGoogleTagManagerType" type="config">
        <data key="path">google/analytics/type</data>
        <data key="value">tag_manager</data>
    </entity>
</entities>
