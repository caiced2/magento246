<actionGroups xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="urn:magento:mftf:Test/etc/actionGroupSchema.xsd">
    <actionGroup name="AdminCustomerSegmentAddSKUConditionActionGroup" extends="AdminCustomerSegmentAddConditionActionGroup">
        <arguments>
            <argument name="conditionName" defaultValue="{{CustomerSegmentProductSKUCondition.name}}" type="string"/>
            <argument name="defaultConditionOperator" defaultValue="{{CustomerSegmentProductSKUCondition.operator}}" type="string"/>
            <argument name="conditionOperator" defaultValue="{{CustomerSegmentProductSKUCondition.operator}}" type="string"/>
            <argument name="defaultConditionOperand" defaultValue="{{CustomerSegmentProductSKUCondition.operand}}" type="string"/>
            <argument name="conditionOperand" defaultValue="" type="string"/>
        </arguments>
    </actionGroup>
</actionGroups>
