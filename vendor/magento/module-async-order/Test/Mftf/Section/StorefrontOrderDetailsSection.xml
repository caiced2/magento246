<sections xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:noNamespaceSchemaLocation="urn:magento:mftf:Page/etc/SectionObject.xsd">
    <section name="StorefrontOrderDetailsSection">
        <element name="status" type="text" selector="//span[contains(text(),'{{var1}}')]" parameterized="true"/>
        <element name="reorderButton" type="button" selector="//a/span[contains(text(),'Reorder')]"/>
        <element name="priceExcludingTax" type="text" selector="td.price span.price"/>
     </section>
</sections>
