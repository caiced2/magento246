<sections xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:noNamespaceSchemaLocation="urn:magento:mftf:Page/etc/SectionObject.xsd">
    <section name="AdminScheduledImportsExportsSection">
        <element name="actionsDropDown" type="select" selector="#operationGrid_massaction-select"/>
        <element name="submitButton" type="button" selector="[title=Submit]"/>
        <element name="massActionDropDown" type="select" selector="#operationGrid_massaction-mass-select"/>
        <element name="nthRow" type="text" parameterized="true" selector="#operationGrid_table tbody tr:nth-of-type({{rowIndex}})"/>
        <element name="checkboxByRow" type="input" parameterized="true" selector="#operationGrid_table tbody tr:nth-of-type({{rowIndex}}) [data-column=massaction] input"/>
        <element name="nameByRow" type="text" parameterized="true" selector="#operationGrid_table tbody tr:nth-of-type({{rowIndex}}) [data-column=name]"/>
        <element name="operationByRow" type="text" parameterized="true" selector="#operationGrid_table tbody tr:nth-of-type({{rowIndex}}) [data-column=operation_type]"/>
        <element name="entityTypeByRow" type="text" parameterized="true" selector="#operationGrid_table tbody tr:nth-of-type({{rowIndex}}) [data-column=entity_type]"/>
        <element name="lastRunByRow" type="text" parameterized="true" selector="#operationGrid_table tbody tr:nth-of-type({{rowIndex}}) [data-column=last_run_date]"/>
        <element name="frequencyByRow" type="text" parameterized="true" selector="#operationGrid_table tbody tr:nth-of-type({{rowIndex}}) [data-column=freq]"/>
        <element name="statusByRow" type="text" parameterized="true" selector="#operationGrid_table tbody tr:nth-of-type({{rowIndex}}) [data-column=status]"/>
        <element name="lastOutcomeByRow" type="text" parameterized="true" selector="#operationGrid_table tbody tr:nth-of-type({{rowIndex}}) [data-column=is_success]"/>
        <element name="actionDropDownByRow" type="select" parameterized="true" selector="#operationGrid_table tbody tr:nth-of-type({{rowIndex}}) [data-column=action] select"/>
    </section>
</sections>
