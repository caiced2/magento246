<actionGroups xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:noNamespaceSchemaLocation="urn:magento:mftf:Test/etc/actionGroupSchema.xsd">
    <actionGroup name="AdminOpenScheduledExportCreatePageActionGroup">
        <annotations>
            <description>Open Scheduled export create page.</description>
        </annotations>

        <amOnPage url="{{AdminScheduledExportEditPage.url}}" stepKey="goToExportCreatePage"/>
        <waitForPageLoad stepKey="waitForCreateExportPagePage"/>
    </actionGroup>
</actionGroups>
