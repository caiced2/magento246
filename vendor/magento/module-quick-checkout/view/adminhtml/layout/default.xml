<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd" >
    <body>
        <referenceContainer name="header">
            <block name="quickCheckoutTracking"
                   template="Magento_QuickCheckout::tracking.phtml"
                   ifconfig="admin/usage/enabled">
                <arguments>
                    <argument name="metadata" xsi:type="object">Magento\QuickCheckout\ViewModel\Metadata</argument>
                </arguments>
            </block>
        </referenceContainer>
    </body>
</page>


