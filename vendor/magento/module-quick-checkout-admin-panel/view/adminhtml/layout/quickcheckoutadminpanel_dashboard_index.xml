<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="page.title">
            <action method="setPageTitle">
                <argument name="title" translate="true" xsi:type="string">Quick Checkout</argument>
            </action>
        </referenceBlock>
        <referenceContainer name="content">
            <block class="Magento\QuickCheckoutAdminPanel\Block\Adminhtml\Index" name="quickcheckoutadminpanel.index" template="Magento_QuickCheckoutAdminPanel::index.phtml"/>
        </referenceContainer>
    </body>
</page>
